<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.knewhow.springcloud.mapper.CollegeMapper">
    <select id="queryAll" resultType="College">
        select * from college;
    </select>
    
    <select id="queryByID" resultType="College">
        select * from college where id = #{ID}
    </select>

</mapper>